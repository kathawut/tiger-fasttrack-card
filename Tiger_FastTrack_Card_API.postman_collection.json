					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Extract and save JWT token",
									"if (pm.response.code === 200) {",
									"    const response = pm.response.json();",
									"    if (response.token) {",
									"        pm.collectionVariables.set(\"authToken\", response.token);",
									"        console.log(\"Auth token saved:\", response.token);",
